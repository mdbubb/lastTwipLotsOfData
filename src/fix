import java.io.File;
import java.io.IOException;
import java.util.InputMismatchException;
import java.util.Scanner;

/**
 * Created by mb977 on 4/20/17.
 */
public class fix {
    public static void fix1(String[] dd, int n) {
        System.out.println(n);
        Main.c = new double[dd.length];
        for (int i = 1; i < dd.length; i++) {
            Main.c[i] = Double.parseDouble(dd[i]);
            System.out.println(Main.c[i]);
            Main.p += Main.c[i];
        }

            Main.sub = n * .25;
            Main.fr = Main.p-Main.sub ;
            System.out.print("Total points: ");
            System.out.println(Main.fr + " \n");




    }

    public static void getAverage() throws IOException {
        Scanner total = new Scanner(new File("totals"));
        while (total.hasNextLine()) {
            int id;
            double totalllll;
            total.next();
            id = total.nextInt();
            total.next();
            totalllll = total.nextDouble();
            System.out.println(totalllll);


        }
    }
}
